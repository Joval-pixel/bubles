{
  "cleanUrls": true,
  "trailingSlash": false,

  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.bubles.com.br" }],
      "destination": "https://bubles.com.br/:path*",
      "permanent": true
    }
  ],

  "headers": [
    {
      "source": "/:all*(css|js|png|jpg|jpeg|gif|svg|webp|ico|woff|woff2|ttf)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/:path*",
      "headers": [
        { "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains; preload" }
      ]
    }
  ],

